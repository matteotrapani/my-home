{
  "name": "my-home",
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c backend/tslint.json -p backend/tsconfig.json --fix",
    "ng": "ng",
    "dev-server": "nodemon",
    "start": "node dist/index.js",
    "build": "tsc --project backend/tsconfig.json --allowSyntheticDefaultImports --outDir dist && cpx backend/src/server.key dist && cpx backend/src/server.crt dist && cd frontend && ng build --prod --output-path ../dist/frontend",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.7",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.7",
