    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "morgan": "^1.10.0",
    "protractor": "^5.4.4",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.2",
    "typescript": "~3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
